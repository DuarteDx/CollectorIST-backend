{
  "presets": [
    ["env", { "targets": { "node": "current" }}],
    "stage-3"
  ],
  "plugins": [
    ["babel-plugin-root-import", {
      "rootPathPrefix": "@",
      "rootPathSuffix": "src"
    }],
    ["babel-plugin-root-import", {
      "rootPathPrefix": "#",
      "rootPathSuffix": "test"
    }],
    ["add-module-exports"]
  ]
}